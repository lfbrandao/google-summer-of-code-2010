<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.email %></td>
    <td><%= link_to 'Edit', edit_user_registration_path %></td>
    <td><%= link_to 'Destroy', :controller => :users, :method => :delete, :id => @user %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_registration_path %>